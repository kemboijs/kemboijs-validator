{
  "name": "nodejs-validator",
  "version": "0.0.1",
  "description": "This is nodejs-validator middleware build from validator.js",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=text mocha --require ts-node/register --watch-extensions ts,tsx test/**/*.{ts,tsx} [...args]",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kemboijs/kemboijs-validator.git"
  },
  "keywords": [
    "nodejs",
    "validator",
    "validatorjs",
    "kemboijs",
    "nodejs",
    "middleware"
  ],
  "author": "Ezrqn Kemboi <ezrqnkemboi@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kemboijs/kemboijs-validator/issues"
  },
  "homepage": "https://github.com/kemboijs/kemboijs-validator#readme",
  "dependencies": {
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ]
  }
}